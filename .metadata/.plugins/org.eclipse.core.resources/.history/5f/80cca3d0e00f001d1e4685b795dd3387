package statements;

public class S03_Calc {
	public static void main(String[] args) {
		
		int a = 20;
		int b = 50;
		String choice = "mul";
		
		if(choice=="add")
		{
			int c = a+b;
			System.out.println("Addition is : "+(a+b));
		}
		else if(choice=="sub")
		{
			int c = a-b;
			System.out.println("substraction is : "+(a-b));
		}
		else if(choice=="mul")
		{
			int c = a*b;
			System.out.println("Multiplication is : "+(a*b));
		}
		
		
		
	}
}
