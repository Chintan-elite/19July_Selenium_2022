package oops;

class Student
{
	Student()
	{
		System.out.println("constructor calling....");
	}
	
	Student(int i)
	{
		System.out.println(i);
	}
}

public class O002_ConstructorDemo {
	public static void main(String[] args) {
		
		
		Student st = new Student();
		Student st1 = new Student(10);
		
		
		
	}
}
